{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "endOfLine": "auto",
  "jsxSingleQuote": true,

  "importOrder": [
    "^react(.*)",
    "^redux/(.*)$",
    "^@reduxjs/(.*)$",
    "^@tanstack/(.*)$",
    "^@apis/(.*)$",
    "^@hooks/(.*)$",
    "^@common/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^styles/(.*)$",
    "^[./]",
    "^types",
    "<THIRD_PARTY_MODULES>"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
